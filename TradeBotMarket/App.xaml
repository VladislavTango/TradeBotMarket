<Application x:Class="TradeBotMarket.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TradeBotMarket"
             xmlns:s="clr-namespace:TradeBotMarket.Views.Pages"
             xmlns:k="clr-namespace:TradeBotMarket.ViewModels">
    <Application.Resources>
        <DataTemplate DataType="{x:Type k:TradePageViewModel}">
            <s:TradePage/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type k:CandleViewModel}">
            <s:CandlePage/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type k:BalanceViewModel}">
            <s:BalancePage/>
        </DataTemplate>
    </Application.Resources>
</Application>
